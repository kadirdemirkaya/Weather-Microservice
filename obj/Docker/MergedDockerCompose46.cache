    command:
    - agent
    - -dev
    - -client=0.0.0.0
    container_name: consul
    image: consul
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 8500
      published: "8500"
      protocol: tcp
    - mode: ingress
      target: 8600
      published: "8600"
      protocol: udp
